journal = {The Journal of the Acoustical Society of America},
number = {3_Supplement},
pages = {1714--1714},
publisher = {pubs.aip.org},
title = {{Fractal and bio-inspired labyrinthine acoustic metamaterials}},
url = {https://pubs.aip.org/asa/jasa/article-abstract/143/3_Supplement/1714/679889},
volume = {143},
year = {2018}

@article{jalali2023micromechanics,
abstract = {The present report proposes a micromechanics model for predicting the dynamic modulus of rubber blends consisting of a base rubber and devulcanized recycled rubbers reinforced by nanofillers. A four-step hierarchical homogenization is established to relate the properties of components from the nano-sized reinforcements to the macro-scaled final blend. The developed model accounts for the effect of agglomeration and geometry of nano-fillers, the quality of the devulcanization process, and the properties of the interphase surrounding the nanofillers on the dynamic modulus of the rubber blend. A comprehensive dimensionless parametric study is performed whose output in terms of graphical maps can be used as a design guideline for practical cases in industries like automotive. A case study for recycled end-of-life tires (ELTs) reinforced by nano clays is presented and it is found that the nano reinforcement of devulcanized rubbers has a remarkable potential to compensate for the weakness compared to its virgin counterpart.},
author = {Jalali, Seyed Kamal and Busfield, James and Pugno, Nicola},
doi = {10.1007/s12239-023-0080-z},
issn = {19763832},
journal = {International Journal of Automotive Technology},
keywords = {Devulcanization,End-of-life tires (ELTs),Halpin-Tsai (H-T),Nano reinforcement,Recycled rubber},
number = {4},
pages = {983--994},
publisher = {Springer},
title = {{Micromechanics Model for Rubber Blends Filled by a Nano-Reinforced Devulcanized Recycled Rubber: Application in the Automotive Industry}},
volume = {24},
year = {2023}
}

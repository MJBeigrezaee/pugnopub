journal = {Acta Astronautica},
keywords = {Artsutanov,Flaw tolerant,Graphene,Nanotubes,Self-collapse,Space elevator},
number = {2},
pages = {221--224},
title = {{Towards the Artsutanov's dream of the space elevator: The ultimate design of a 35 GPa strong tether thanks to graphene}},
volume = {82},
year = {2013}
}
@article{pop00621,
abstract = {Hook-like surface structures, observed in some plant species, play an important role in the process of plant growth and seed dispersal. In this study, we developed an elastic model and further used it to investigate the mechanical behaviour of fruit hooks in four plant species, previously measured in an experimental study. Based on Euler-Bernoulli beam theory, the force- displacement relationship is derived, and its Young's modulus is obtained. The result agrees well with the experimental data. The model aids in understanding the mechanics of hooks, and could be used in the development of new bioinspired Velcro-like materials. {\textcopyright} 2013 The Authors.},

keywords = {Electrical properties,Fracture,Mechanical properties,Nano composites},
pages = {125--131},
publisher = {Elsevier},
title = {{Severe graphene nanoplatelets aggregation as building block for the preparation of negative temperature coefficient and healable silicone rubber composites}},

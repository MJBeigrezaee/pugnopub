eprint = {2104.02796},
issn = {16136829},
journal = {Small},
keywords = {carbon nanotubes,graphene nanoribbons,modeling,molecular dynamics,nano-oscillator},
number = {35},
pmid = {34302438},
publisher = {Wiley Online Library},
title = {{Controlling Movement at Nanoscale: Curvature Driven Mechanotaxis}},

keywords = {Atomistic modelling,Carbon foams,Equation of state,Graphene nanocomposites,Hypervelocity shock,Interatomic forces},
publisher = {Elsevier},
title = {{Atomistic modelling of the hypervelocity dynamics of shock-compressed graphite and impacted graphene armours}},

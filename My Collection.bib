@article{pop00510,
abstract = {The recent isolation of graphene from graphite has led to the discovery and synthesis of a multitude of similar two-dimensional (2D) crystalline and has ushered in a new era of 2D materials. These single-atom-thick materials have shown great promise for emerging nanotechnological applications, with unique electrical, thermal, and optical properties. Underlying their success as a platform for nanosystem development is their structural integrity, mechanical stability, and limit states in loading and deformation. As such, a full understanding of their mechanistic behavior, properties, and failure modes is essential for the successful implementation of 2D materials in design. Here, we focus on the fundamental nanomechanics of 2D materials, describing how to characterize basic properties such as strength, stiffness, bending rigidity, and adhesion using computational methods such as molecular dynamics (MD). Failure is discussed in terms of the local, atomistic interpretation of quantized fracture mechanics (QFM), and extended to bulk systems via nanoscale Weibull statistics (NWS). While 2D materials have shown promise in nanotechnology, in terms of complete understanding of the mechanical behavior of these systems, we have merely scratched the surface.},
annote = {Query date: 2025-01-07 19:31:12},
author = {Roman, Ruth E. and Pugno, Nicola M. and Cranford, Steven W.},
doi = {10.1002/9781119068921.ch8},
isbn = {9781119068921},
journal = {Advanced Computational Nanomechanics},
keywords = {2D materials,2D modulus,Adhesion,Bending rigidity,Folding,Graphene,Molecular dynamics,Nanoscale Weibull statistics,Quantized fracture mechanics,Virial stress},
pages = {201--242},
publisher = {Wiley Online Library},
title = {{Mechanical Characterization of 2D Nanomaterials and Composites}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119068921.ch8},
year = {2016}
}

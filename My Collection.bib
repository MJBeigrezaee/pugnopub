journal = {International Journal of Engineering Science},
keywords = {Adhesion,Griffith approach,Multiscale models,Phase Transition,Statistical Mechanics,Thermal effects},
publisher = {Elsevier},
title = {{Exploring the impact of thermal fluctuations on continuous models of adhesion}},
